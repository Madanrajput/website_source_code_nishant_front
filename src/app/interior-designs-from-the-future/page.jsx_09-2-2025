                          type="text"
                          className="form-control"
                          id="validationCustom01"
                          placeholder="Name"
                          name="fullName"
                          onChange={handleInputChange}
                          value={formData.fullName}
                          required
                        />
                      </div>
                      <div className="col-md-5 mb-3">
                        <input
                          type="text"
                          className="form-control"
                          id="validationCustom05"
